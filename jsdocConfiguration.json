{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "remoteFileExplorer",
    "footer": "Created by Jens Heidl",
    "copyright": "2016",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "doc",
    "recurse": true,
    "private": true
  }
}